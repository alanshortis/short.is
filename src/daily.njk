<div>
  {%- for post in collections.daily | reverse -%}
    <a href="{{post.data.permalink}}">
      <p>
        <span class="sr-only">Posted on: </span>
        <time datetime="{{ post.data.date }}">{{ post.data.date | formatDate }}</time>
      </p>
      <p>{{post.data.title}}</p>
    </a>
  {%- endfor %}
</div>
